@import "global.lvh"
@import "list.lvh"
@import "vector.lvh"

@namespace vector
@using list:Nil
@using list:<::

(def _toList_impl(vec, i)
    => Nil ; i = size(vec)
    => [vec](i) <:: _toList_impl(vec, i + 1) ; rest
)

def Vector(list) => fromList(list)
def i_toList(vector) => _toList_impl(vector, 0)
